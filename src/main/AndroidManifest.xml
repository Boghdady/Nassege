<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.index.Nassege">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/shoppingcart"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shoppingcart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.OurServices" />
        <activity android:name=".Activities.AboutUs" />
        <activity android:name=".Activities.ContactUS" />
        <activity android:name=".Activities.AircraftServices" />
        <activity android:name=".Activities.ConsultationServices"></activity>
    </application>

</manifest>